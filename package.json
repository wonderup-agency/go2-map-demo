    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup": "^4.47.1",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.14.3",
