{
  "name": "starter",
  "author": "Napu ðŸ˜Ž",
  "description": "A minimal JavaScript starter project with Rollup, PostCSS, and optimized build tooling for modern web development.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonderup-agency/starter"
  },
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "server": "http-server -c-1 --cors --proxy http://127.0.0.1:8080? ./dist",
